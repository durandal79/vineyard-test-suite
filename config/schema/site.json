{
  "tables": {
    "objects_tags": {
      "primary_keys": [ "object", "tag" ],
      "properties": {
        "object": {
          "type": "int"
        },
        "tag": {
          "type": "int"
        }
      }
    }
  },
  "trellises": {
    "object": {
      "properties": {
        "tags": {
          "type": "list",
          "trellis": "tag"
        }
      }
    },
    "character": {
      "parent": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "is_alive": {
          "type": "bool",
          "default": true
        },
        "all": {
          "type": "string",
          "is_virtual": true
        },
        "items": {
          "type": "list",
          "trellis": "item"
        }
      }
    },
    "item": {
      "parent": "object",
      "properties": {
        "owner": {
          "type": "reference",
          "trellis": "character"
        }
      }
    },
    "tag": {
      "properties": {
        "id": {
          "type": "int"
        },
        "name": {
          "type": "string"
        },
        "objects": {
          "type": "list",
          "trellis": "object"
        }
      }
    }
  }
}